{
	"name": "snapgram",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db": "drizzle-kit generate:mysql --schema=./src/lib/server/schema.ts && tsx ./src/lib/server/migrate.ts",
		"seed": "tsx ./src/lib/utils/seeding.ts"
	},
	"devDependencies": {
		"@faker-js/faker": "^8.3.1",
		"@sveltejs/adapter-auto": "^3.0.0",
