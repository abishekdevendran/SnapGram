		"@uppy/transloadit": "^3.4.0",
		"bits-ui": "^0.11.8",
		"cloudinary": "^1.41.1",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.29.1",
		"lucia": "^2.7.5",
		"lucide-svelte": "^0.299.0",
		"minio": "^7.1.3",
